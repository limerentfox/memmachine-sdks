{
  "name": "@instruct/memmachine-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for MemMachine Unified API",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "typescript": "^5.6.3"
  },
  "keywords": [
    "memmachine",
    "sdk",
    "typescript",
    "memory",
    "llm",
    "mcp"
  ],
  "author": "Instruct",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/limerentfox/memmachine-sdks",
    "directory": "packages/js-sdk"
  }
}
